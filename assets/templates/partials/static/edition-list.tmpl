{{/*  Title of list  */}}
<h2>{{ len .Editions }} editions in this series</h2>

{{/*  Editions list  */}}
<ul class="ons-list ons-list--summary ons-list--icons">
    {{ range .Editions }}
        <li class="ons-list__item">
            <a href="{{.LatestVersionURL}}">
                {{ .Title}}
            </a>
        </li>
    {{ end }}
</ul>
