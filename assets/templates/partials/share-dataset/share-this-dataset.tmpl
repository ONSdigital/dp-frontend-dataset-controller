<hr />
<h2 class="u-fw-b u-mb-xxs u-mt-s">Share this dataset</h2>
<ul class="list list--inline list--bare list--icons">
  {{ range . }}
  <li class="list__item u-pl-no u-pr-xs">
    <span class="list__prefix" >
    {{ template "partials/share-dataset/icon-handler" .Icon }}
    </span><a href="{{.Link}}" class="list__link u-fs-r u-pt-xxs underline-link" target="_blank" rel="noreferrer external">{{ .Title }}<span class="u-vh">this link will open in a new tab</span></a>
  </li>
  {{ end }}
</ul>
